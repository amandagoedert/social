{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/ironpay_webhook",
      "dest": "/api/ironpay_webhook.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

